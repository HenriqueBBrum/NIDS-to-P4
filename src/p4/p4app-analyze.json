{
  "program": "p4snort.p4",
  "language": "p4-16",
  "targets": {
  "multiswitch": {
      "links": [
        ["h1", "s1"],
        ["h2", "s1"],
        ["h3", "s1"]
      ],
      "auto-control-plane": true,
      "controller_module": "mycontroller",
      "num-hosts": 3,
      "bmv2_log": true,
      "pcap_dump": false,
      "switches": {
        "s1": {
          "commands": "p4snort.config"
        }
      },
      "hosts": {
        "h1": {
          "cmd": "snort -A console -c /etc/snort/snort.conf -r /pcaps/$pcap_file 2>&1",
          "wait": true
        }
      },
      "parameters": {
        "generator_interface_name": "h3-eth0",
        "s1_interface_name": "s1-eth1",
        "snort_interface_name": "h1-eth0",
        "pcap_file": "Friday-WorkingHours.pcap"
      }
    }
  }
}